<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="MemberCar">
	<sql id="queryListByPage"> 
 	<![CDATA[ 
		select 
			   mc.id
			  ,mc.memberId
			  ,mc.carNumber
			  ,mc.carType
			  ,mc.load
			  ,mc.carLength
			  ,mc.vehicleUrl
			  ,mc.carHeadUrl
			  ,mc.carRearUrl
			  ,mc.isDeleted
			  ,mc.createUserId
			  ,mc.createTime
			  ,mc.updateUserId
			  ,mc.updateTime
			  ,mi.userName
			  ,mi.mobile
			  ,mi.serviceType
			  from member_car mc
			  left join member_info mi
			  on mi.id=mc.memberId
			  where  1=1
			  <#if carNumber?exists && carNumber!="" >
			  and mc.carNumber  like  "%":carNumber"%"
			  </#if>
			  <#if userName?exists && userName!="" >
			    and mi.userName  like  "%":userName"%"
			  </#if>
			  <#if carType?exists && carType!="" >
			   and mc.carType=:carType
			  </#if>
			  <#if isDeleted?exists && isDeleted!="" >
			   and mc.isDeleted=:isDeleted
			  </#if>
			  <#if startDate?exists && startDate!="" >
			    and mc.createTime>=:startDate
			  </#if>
			  <#if endDate?exists && endDate!="" >
			   and mc.createTime<=:endDate
			  </#if>
			   <#if mobile?exists && mobile!="" >
			    and mi.mobile  like  "%":mobile"%"
			  </#if>
			  <#if memberId?exists && memberId!="" >
			    and mc.memberId=:memberId
			  </#if>
			 <#if serviceType?exists && serviceType!="" >
			    and mi.serviceType=:serviceType
			  </#if>
			  order by mc.createTime desc
			   LIMIT :startRow,:endRow
 	]]> 
 	</sql>
 	
 	<sql id="countTotal"> 
 	   <![CDATA[ 
		     select 
			  count(1)
			   from member_car mc
			  left join member_info mi
			  on mi.id=mc.memberId
			  where  1=1
			  <#if carNumber?exists && carNumber!="" >
			  and mc.carNumber  like  "%":carNumber"%"
			  </#if>
			  <#if userName?exists && userName!="" >
			    and mi.userName  like  "%":userName"%"
			  </#if>
			  <#if carType?exists && carType!="" >
			   and mc.carType=:carType
			  </#if>
			  <#if isDeleted?exists && isDeleted!="" >
			   and mc.isDeleted=:isDeleted
			  </#if>
			  <#if startDate?exists && startDate!="" >
			    and mc.createTime>=:startDate
			  </#if>
			  <#if endDate?exists && endDate!="" >
			   and mc.createTime<=:endDate
			  </#if>
			   <#if mobile?exists && mobile!="" >
			    and mi.mobile  like  "%":mobile"%"
			  </#if>
			    <#if memberId?exists && memberId!="" >
			    and mc.memberId=:memberId
			  </#if>
			   <#if serviceType?exists && serviceType!="" >
			    and mi.serviceType=:serviceType
			  </#if>
 				]]> 
 	      </sql>
 	      
 	      
 	      <sql id="carNumberCountTotal"> 
 	   <![CDATA[ 
		     select 
			  count(1)
			   from member_car mc
			  left join member_info mi
			  on mi.id=mc.memberId
			  where  1=1
			  <#if carNumber?exists && carNumber!="" >
			  and mc.carNumber  =:carNumber
			  </#if>
			  <#if id?exists && id!="" >
			   and mc.id!=:id
			  </#if>
			
 				]]> 
 	      </sql>
 	
 	<sql id="queryMemberCarDetail"> 
 	<![CDATA[ 
		select 
			   mc.id
			  ,mc.memberId
			  ,mc.carNumber
			  ,mc.carType
			  ,mc.load
			  ,mc.carLength
			  ,mc.vehicleUrl
			  ,mc.carHeadUrl
			  ,mc.carRearUrl
			  ,mc.isDeleted
			  ,mc.createUserId
			  ,mc.createTime
			  ,mc.updateUserId
			  ,mc.updateTime
			  ,mi.userName
			  ,mi.mobile
			  from member_car mc
			  left join member_info mi
			  on mi.id=mc.memberId
			  where  mc.id=:id
 	]]> 
 	</sql>
 	<sql id="updateMemberCarById"> 
 	<![CDATA[
 	update  member_car  
 	set 
	 	<#if updateUserId?exists && updateUserId!="" >
				 updateUserId=:updateUserId,
		</#if>
		<#if load?exists && load!="" >
				 `load`=:load,
		</#if>
		<#if carNumber?exists && carNumber!="" >
				 carNumber=:carNumber,
		</#if>
		<#if carType?exists && carType!="" >
				 carType=:carType,
		</#if>
		<#if carLength?exists && carLength!="" >
				 carLength=:carLength,
		</#if>
	 	<#if isDeleted?exists && isDeleted!="" >
				 isDeleted=:isDeleted,
		</#if>
 	updateTime=now()
 	where  id=:id
 	]]>
 	</sql>
 
</sqlMap>
