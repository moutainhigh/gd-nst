			</#if>
		</#if>
 	]]> 
 	</sql>
 	
 	<sql id="getAdminOrderDTOBySourceId">
 	<![CDATA[ 
 		SELECT
			oi.orderNo,
			oi.orderStatus,
			oi.payStatus,
			oi.transportAmt as payMoney,
		  	shipper.userName as buyerName,
			shipper.mobile as buyerMobile,
			driver.userName as sellerName,
			driver.mobile as sellerMobile
		FROM
			order_info oi
		LEFT JOIN
			member_info driver ON driver.id = oi.driverMemberId
		LEFT JOIN
			member_info shipper ON shipper.id = oi.shipperMemberId
		WHERE
			oi.sourceId = :sourceId
 	]]>
 	</sql>
 	
 	<sql id="getById">
 	<![CDATA[
	 	SELECT
			oi.id,
			oi.orderNo,
			oi.confirmOrderTime,
			oi.confirmGoodsTime,
			oi.createTime,
			oi.orderStatus,
			oi.payStatus,
			oi.transportAmt,
			driver.userName as driverName,
			driver.mobile as driverMobile,
			source.shipperName,
			source.shipperMobile,
			source.sourceType,
			source.s_detail as sDetail,
			source.e_detail as eDetail,
			source.s_detailed_address as sDetailedAddress,
			source.e_detailed_address as eDetailedAddress,
			source.sendDate,
			source.carType,
			source.carLength,
			source.sendGoodsType,
			source.freight,
			source.remark,
			source.goodsType,
			source.goodsName,
			source.totalWeight,
			source.totalSize,
			mc.companyName as logisticName,
			logistic.mobile as logisticMobile,
			(SELECT createTime FROM order_before WHERE id = oi.orderBeforeId) as acceptTime
		FROM(
			SELECT
				id,
				orderBeforeId,
				orderNo,
				orderStatus,
				payStatus,
				sourceId,
				transportAmt,
				shipperMemberId,
				driverMemberId,
				confirmOrderTime,
				confirmGoodsTime,
				createTime
			FROM
				order_info
			WHERE
				id = :id
		) as oi
		LEFT JOIN
			member_info driver ON driver.id = oi.driverMemberId
		LEFT JOIN
			source_shipper source ON source.id = oi.sourceId
		LEFT JOIN
			member_info logistic ON logistic.id = source.assignMemberId
		LEFT JOIN
			member_cer mc ON mc.memberId = source.assignMemberId AND mc.userType = 2
 	]]>
 	</sql>
 	
 	<sql id="queryListForExport"> 
 	<![CDATA[ 
        SELECT
			oi.id,
			oi.orderNo,
			ss.sourceType,
			oi.createTime,
			oi.orderStatus,
			oi.payStatus,
			oi.transportAmt,
			ss.s_detail as sDetail,
			ss.e_detail as eDetail,
			ss.s_detailed_address as sDetailedAddress,
			ss.e_detailed_address as eDetailedAddress,
			ss.goodsType,
			ss.totalWeight,
			ss.sourceType,
			ss.sendGoodsType,
			ss.freight,
			shipper.userName as shipperName,
			shipper.mobile as shipperMobile,
		  	driver.userName as driverName,
			driver.mobile as driverMobile
		FROM
			order_info oi
		LEFT JOIN
			source_shipper ss ON ss.id = oi.sourceId
		LEFT JOIN
			member_info shipper ON shipper.id = oi.shipperMemberId
		LEFT JOIN
			member_info driver ON driver.id = oi.driverMemberId 
		WHERE
			1 = 1
		<#if id?exists && id != "">
			AND oi.id < :id
		</#if>
		<#if orderNo?exists && orderNo != "">
