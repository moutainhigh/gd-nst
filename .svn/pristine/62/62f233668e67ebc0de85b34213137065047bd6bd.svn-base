.f666{color:#666666;}

/*联系我们*/
.contactUs{width:100%;color:#999999;}
.contactUs .header{width:100%;border-bottom:1px dashed #cccccc;font-size:1.7rem;background-color:#ffffff;margin:1rem 0 0 0;}
.contactUs .header .top{border-bottom:1px solid #CCCCCC;padding:0 3%;line-height:6rem;position:relative;}
.contactUs .header .top img{position:absolute;width:30%;top:0;right:0;}
.contactUs .header .top span{color: #ff6c00;font-size:2rem;}
.contactUs .header .bottom{padding:1.6rem 3%;}
.contactUs .header .bottom p{margin:.5rem 0;}
.contactUs .con{width:94%;background-color:#FFFFFF;padding:1.6rem 3%;font-size:1.7rem;line-height:3rem;}
.contactUs .con li{width:100%;padding:1rem 0;overflow:hidden;}
.contactUs .con li .left{float:left;width:23%;}
.contactUs .con li .right{color:#666666;font-size:1.9rem;display: block;float: left;width:75%;}
.contactUs a{width:95%;display: block;position: fixed;bottom:1rem;left:2.5%;line-height:4rem;font-size:2rem;color:#FFFFFF;background-color:#3fad4f;text-align: center;border-radius:5px;}




