@charset "utf-8";
ul,
ol,
dl,
dd,
h1,
h2,
h3,
h4,
h5,
h6,
figure,
form,
input,
textarea,
button,
p,
th,
td,
body,
html
{
    margin: 0;
    padding: 0;
}

/*// 1. 为不继承的元素设定继承父级行高*/
/*// 2. 为不继承的元素设定继承父级字体*/
input,
textarea,
button{
    line-height: inherit;
    font-family: inherit;
}
/*清除浮动*/
.clearfix::after,
.clearfix::before{
    clear: both;
    content: '.';
    height: 0;
    line-height: 0;
    visibility: hidden;
    display: block;
}

h1,
h2,
h3,
h4,
h5,
h6,
input,
textarea,
button
{
    font-size: inherit;
}

/*// 1. 重置斜体表现为正常文本*/
em,
i
{
    font-style: normal;
}

/*// 1. 清除单元格边距并让边框合并*/
table {
    border-collapse: collapse;
    border-spacing: 0;
    table-layout: fixed;
    text-align: left;
}

/*// 1. 清除列表默认序号*/
ul,
ol,
menu {
    list-style: none;
}

/*// 1. 清除默认边框*/
/*// 2. 清除img底部留白问题，并使得默认居中*/
fieldset,
img {
    display: block;
    border: 0;

}

/*// 1. 设置HTML5部分新增元素为块元素*/
article,
aside,
figcaption,
figure,
footer,
header,
nav,
section
{
    display: block;
}

/*// 1. 设置HTML5新增媒体元素与img, object保持一致*/
audio,canvas,video {  
    display: inline-block;*display: inline;*zoom: 1;  
} 


/*// 1. 设置文本域滚不可拖动改变垂直方向尺寸*/
textarea {
    resize:none;
}
/*// 1. 清除webkit下表单元素的outline*/
input,
textarea,
button,
select
a {
    outline: 0 none;
}
h1,h2,h3,h4,h5,h6 {font-size:100%;font-weight:normal}
table {border-collapse:collapse;border-spacing:0}
em,i,b{ font-style:normal; font-weight:normal;}
a:focus{outline:0;}

/*// 1. 清除Firefox按钮内部边框和补白*/
button::-moz-focus-inner,
input::-moz-focus-inner {
    padding: 0;
    border: 0;
}

/*// 1. 清除文本线条装饰*/
a {
    text-decoration: none;
}

/*// 1. 禁止长按链接与图片弹出菜单*/
/*// issues: 在iOS8.1下此设置对a仍无效*/
a,
img {
    -webkit-touch-callout: none;
}

/*常用元素設置為怪異模式盒模型*/
div,
header,
footer,
section,
ul,
li,
dl,
dt,
dd{
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -o-box-sizing: border-box;
    -ms-box-sizing: border-box;
    box-sizing: border-box;
}
/*全局字体设置*/
body,
html{
    /* 禁止用户选中页面内容 */
    -webkit-user-select: none;
    height:100%;
    width: 100%;
}
*{word-wrap:break-word}
.img-responsive{max-width:100%; height:auto;}
.alignleft {
      display: inline;
      float: left;
}
.alignright {
      display: inline;
      float: right;
}
.aligncenter {
      clear: both;
      display: block;
      margin:auto;
} 
body {
    font-family: Helvetica Neue, Helvetica, Arial, sans-serif， Microsoft Yahei, Tahoma;  
    -webkit-text-size-adjust: 100%; 
    -webkit-overflow-scrolling: touch;
    overflow-scrolling: touch;
    font-size: .28rem;
    background: #f9f9f9;
    color: #666666;
}

}
