package cn.gdeng.nst.api.dto.source;

import java.io.Serializable;

/**
 * 规则信息dto
 * 
 * @author xiaojun
 *
 */
public class RuleInfoDTO implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -8318266170460175862L;
	/**
	 * 规则Id
	 */
	private int ruleInfoId;
	/**
	 * 绑定规则的物流公司或车主
	 */
	private int memberId;
	/**
	 * 物流公司或车主当天分配权限
	 */
	private int assginLevel;

	public int getRuleInfoId() {
		return ruleInfoId;
	}

	public void setRuleInfoId(int ruleInfoId) {
		this.ruleInfoId = ruleInfoId;
	}

	public int getMemberId() {
		return memberId;
	}

	public void setMemberId(int memberId) {
		this.memberId = memberId;
	}

	public int getAssginLevel() {
		return assginLevel;
	}

	public void setAssginLevel(int assginLevel) {
		this.assginLevel = assginLevel;
	}

}
