			<!-- 根据货源ID 查询 -->
			<sql id="querySourceShipper">
			<![CDATA[
				SELECT
					id,memberId,sourceType,
					s_provinceId as sProvinceId,
					sourceStatus,
					nstRule,
					version,
					assignMemberId,
					e_provinceId as eProvinceId,
					shipperName,
					shipperMobile
				FROM
					source_shipper
				WHERE
					id=:id AND  isDeleted=0
					
			]]>
			</sql>

	

