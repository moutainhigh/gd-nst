			<sql id="updateOrderBeforeStatusById">
				<![CDATA[
					UPDATE order_before
					SET sourceStatus = :orderBeforeStatus, updateTime = now(), updateUserId =:updateUserId
					WHERE 
						id = :orderBeforeId 
						<#if sourceShipperId?exists && sourceShipperId!="" >
							AND sourceId = :sourceShipperId
						</#if>
				]]>
			</sql>


