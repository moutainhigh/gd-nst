package cn.gdeng.nst.api.dto.ad;

import java.io.Serializable;
import java.util.List;

/**
* @author DJB
* @version 创建时间：2016年8月3日 下午7:15:58
* 所有广告Dto
*/

public class AdAllDto implements Serializable {

	private static final long serialVersionUID = 1867426230007971159L;
	
	private List<AdvertisementDto> adBannerDtos;
	
	private String notice;

	public List<AdvertisementDto> getAdBannerDtos() {
		return adBannerDtos;
	}

	public void setAdBannerDtos(List<AdvertisementDto> adBannerDtos) {
		this.adBannerDtos = adBannerDtos;
	}

	public String getNotice() {
		return notice;
	}

	public void setNotice(String notice) {
		this.notice = notice;
	}
	
	
	

}
