</#if>
 WHERE id=:id
)
]]>
</sql>
<sql id="queryByCondition">
<![CDATA[
	SELECT 
createTime,transStatus
	FROM 
order_info_trans WHERE 
 orderInfoId=:orderInfoId
order by id asc
]]>
</sql>
