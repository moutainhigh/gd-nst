<sqlMap namespace="OrderInfoTrans">
<sql id="insert">
<![CDATA[
	INSERT INTO
order_info_trans(

id,updateTime,transStatus,createTime,createUserId,updateUserId,orderInfoId,closeReason,
operateTime,isDeleted,orderNo,sourceId
) VALUES (

:id,:updateTime,:transStatus,:createTime,:createUserId,:updateUserId,:orderInfoId,:closeReason,
:operateTime,:isDeleted,:orderNo,:sourceId
)
]]>
</sql>

<sql id="update">
<![CDATA[
	UPDATE order_info_trans
 SET 

<#if id?exists && id!="" >
id=:id,
</#if>
<#if updateTime?exists && updateTime!="" >
updateTime=:updateTime,
</#if>
<#if transStatus?exists && transStatus!="" >
transStatus=:transStatus,
</#if>
<#if createTime?exists && createTime!="" >
createTime=:createTime,
</#if>
<#if createUserId?exists && createUserId!="" >
createUserId=:createUserId,
</#if>
<#if updateUserId?exists && updateUserId!="" >
updateUserId=:updateUserId,
</#if>
<#if orderInfoId?exists && orderInfoId!="" >
orderInfoId=:orderInfoId,
</#if>
<#if closeReason?exists && closeReason!="" >
closeReason=:closeReason,
</#if>

<#if operateTime?exists && operateTime!="" >
operateTime=:operateTime,
</#if>
<#if isDeleted?exists && isDeleted!="" >
isDeleted=:isDeleted,
</#if>
<#if orderNo?exists && orderNo!="" >
