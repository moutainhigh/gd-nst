<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="SourceAssignHis">
	<sql id="queryList"> 
 	<![CDATA[ 
        SELECT
			sah.sourceId,
			sah.createTime,
			sah.status,
			sah.updateTime,
            CASE 
				WHEN sah.memberType=2 THEN mi.userName
             ELSE 
                (SELECT companyName  FROM member_cer WHERE memberId = sah.assignMemberId AND userType = 2 LIMIT 1) 
			 END as assignMemberCompanyName,
			 mi.mobile AS assignMemberMobile
		FROM
			source_assign_his sah
		LEFT JOIN member_info mi on mi.id=sah.assignMemberId
		WHERE
			sah.status > 1 AND sah.sourceId = :sourceId
 	]]> 
 	</sql>
 	
</sqlMap>
